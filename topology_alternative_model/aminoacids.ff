;; GLUTAMINE - M3
[ moleculetype ]
; molname       nrexcl
GLM3               1

[ atoms ]
;id type resnr residu atom cgnr   charge
 1   P2                       1     GLM3     BB     1      0
 2   P3                       1     GLM3     SC1    2      0  ;  GLN is P5  but  protein backbone is P2 ? but ASP uncharged is P3  , will 1 oatm less than manuscript version
 3   SN3a                     1     GLM3     SC2    3      0  ; added in response to review 1 -CH2-CH2-    
 4   SN3a                     1     GLM3     SC3    4      0  ; previous SN3a  for ch2-o-ch2  ; in response to review 1
 5   P3                       1     GLM3     SC4    5      0  ;  for ch2-o-ch2
 6   TN4a                     1     GLM3     SC5    6      0  ;  for the smine as review request 
 7   TC5                      1     GLM3     SC6    7    0    ; as review request  three side chains in triangle   ; but use hydrophic type HIS
 8   TC5                      1     GLM3     SC7    8    0    ; as review request  configuration, mimicking
 9   TN4a                     1     GLM3     SC8    9    0    ; as review request  ring structure
 10   P6                      1     GLM3     SC9   10    0     ; termianal hydrophiolic group ....
[bonds]
#meta {"group": "Side chain bonds"}
;  i     j   funct   length  force.c.
  BB   SC1    1       0.470    5000 
  SC1  SC2    1       0.470    5000 ; ; use protein BB-SC1
  SC2  SC3    1       0.315   3200.0 ;  0.370    5000  from b_ET_ET_ether Ether lipid
  SC3  SC4    1       0.470    5000 ; ; use protein BB-SC1
  SC4  SC5    1       0.325    5000 ; ref tyr
  SC8  SC9    1       0.325    5000 ; ref tyr  backbone SC1 side
#meta {"group": "Side chain bonds", "ifdef": "FLEXIBLE"}
;  i     j   funct   length  
 SC5   SC6    1       0.300  $stiff_fc  ; ref tyr
 SC5   SC7    1       0.300  $stiff_fc  ; ref tyr
 SC6   SC8    1       0.285  $stiff_fc  ; ref tyr
 SC7   SC8    1       0.285  $stiff_fc  ; ref tyr
 SC6   SC7    1       0.300  $stiff_fc  
[constraints]
#meta {"group": "Side chain bonds", "ifndef": "FLEXIBLE"}
;  i     j   funct   length  
 SC5   SC6    1       0.300 ; ref tyr
 SC5   SC7    1       0.300 ; ref tyr
 SC6   SC8    1       0.285 ; ref tyr
 SC7   SC8    1       0.285 ; ref tyr
 SC6   SC7    1       0.300 ; ref tyr
[angles]
#meta {"group": "Side chain angles"}
;  i     j    k     funct   angle  force.c.
  BB   SC1  SC2     10 127 20  ; use martini BBB angles   ;   2   120.000   50.0
  SC1  SC2  SC3     1       120.0   20.0 ; ether lipid backbone a_ET_ET_C_ether ;
  SC2  SC3  SC4     1       120.0   20.0 ; ether lipid backbone a_ET_ET_C_ether ;
  SC3  SC4  SC5     1       120.0   20.0 ; ether lipid backbone a_ET_ET_C_ether ;
  SC4  SC5  SC6        2   120.000   50.0  ; TYR
  SC4  SC5  SC7        2   120.000   50.0  ; TYR
;  SC5  SC6  SC8        2   120.000   50.0  
;  SC5  SC7  SC8        2   120.000   50.0
  SC6  SC8  SC9        2   120.000   50.0  ; TYR
  SC7  SC8  SC9        2   120.000   50.0  ; TYR

[dihedrals]
;  i     j    k    l   funct   angle  force.c.
 SC8  SC6  SC7  SC5       2    180.0    50.0     ; to prevent backflipping of ring

[exclusions]
; We do not want non-bounded interactions within the residue.
SC4 SC5 SC6 SC7 SC8 SC9
SC5 SC6 SC7 SC8 SC9
SC6 SC7 SC8 SC9
SC7 SC8 SC9
SC8 SC9


