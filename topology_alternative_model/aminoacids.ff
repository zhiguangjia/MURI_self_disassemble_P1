;;; GLUTAMINE - M1

[ moleculetype ]
; molname       nrexcl
GLM1               1

[ atoms ]
;id type resnr residu atom cgnr   charge
 1   P2                       1     GLM1     BB     1      0
 2   P2                       1     GLM1     SC1    2      0 ; dfault peptide backbone ?  GLN is P5  also see Martini3  paper fir ch2ch2-cooH
                                                             ;  review 1  :  use GLUP  type, bondingf now
 3   SC4                      1    GLM1     SC2    3    0    ; three side chains in triangle
 4   TC5                      1    GLM1     SC3    4    0    ; configuration, mimicking
 5   TC5                      1    GLM1     SC4    5    0    ; ring structure

[bonds]
#meta {"group": "Side chain bonds"}
;  i     j   funct   length  force.c.
  BB   SC1    1      0.400    5000  ;  use GLUP after review 1  0.420    5000  ; 0.42 in aa
 SC1   SC2    1      0.340    5000  ;  PHE standard 0.34 in aa
#meta {"group": "Side chain bonds", "ifdef": "FLEXIBLE"}
;  i     j   funct   length  
 SC2   SC3    1       0.340  $stiff_fc   ; PHE
 SC2   SC4    1       0.340  $stiff_fc   ; PHE
 SC3   SC4    1       0.290  $stiff_fc   ; PHE

[constraints]
#meta {"group": "Side chain bonds", "ifndef": "FLEXIBLE"}
;  i     j   funct   length  
 SC2   SC3    1       0.340
 SC2   SC4    1       0.340
 SC3   SC4    1       0.290

[angles]
#meta {"group": "Side chain angles"}
;  i     j    k     funct   angle  force.c.
  BB   SC1  SC2     10 127 20  ; use martini BBB angles ; 2   120.000   50.0   
  SC1  SC2  SC3        2   120.000   50.0  ; PHE  here SC1 is PHE's BB
  SC1  SC2  SC4        2   120.000   50.0  ; PHE

[exclusions]
; We do not want non-bounded interactions within the residue.
SC1 SC2 SC3 SC4
SC2 SC3 SC4
SC3 SC4


